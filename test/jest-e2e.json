{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@nestjslatam/ddd/(.*)": "<rootDir>/../libs/ddd/src/$1",
    "@nestjslatam/ddd": "<rootDir>/../libs/ddd/src",
    "@app/bus/(.*)": "<rootDir>/../libs/bus/src/$1",
    "@app/bus": "<rootDir>/../libs/bus/src",
    "@app/ddd-shared/(.*)": "<rootDir>/../libs/ddd-shared/src/$1",
    "@app/ddd-shared": "<rootDir>/../libs/ddd-shared/src",
    "@app/ddd-bus/(.*)": "<rootDir>/../libs/ddd-bus/src/$1",
    "@app/ddd-bus": "<rootDir>/../libs/ddd-bus/src"
  }
}